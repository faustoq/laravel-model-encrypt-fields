{
    "name": "faustoq/laravel-model-encrypt-fields",
    "description": "Encrypt and decrypt model fields",
    "keywords": [
        "laravel",
        "package",
        "faustoq",
        "laravel-model-encrypt-fields"
    ],
    "homepage": "https://github.com/faustoq/laravel-model-encrypt-fields",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fausto Quaggia",
            "email": "fausto.quaggia@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ModelEncryptFields\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModelEncryptFields\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModelEncryptFields\\ServiceProvider"
            ]
        }
    }
}